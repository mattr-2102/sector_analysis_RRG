}

#mainContentStackedWidget &gt; QWidget {
	background-color: #26282C;
}

#intersectorPerformancePage, #relativeSectorPerformancePage {
	background-color: #26282C;
}

/* ========== Frame-Like Widgets (e.g. sidebar) ========== */
QFrame {
    background-color: #2F3237;  /* raisin-black */
}

QLabel, QCheckBox {
    background-color: transparent;
}

#mainContentStackedWidget {
	border: 1px solid #26282C;
}


/* ========== Buttons ========== */

QPushButton {
    background-color: #26282C;
    color: #EBEBEB;
    border: 1px solid #42464D;
    padding: 6px 12px;
    border-radius: 4px;
}

QPushButton:hover {
    background-color: #3a3d43;
}
QPushButton:pressed {
    background-color: #5196F5;  /* chefchaouen-blue */
    color: #1C1E21;             /* eerie-black */
}

/* ========== Tool Buttons (e.g., toggle button) ========== */
QToolButton {
    background-color: #2F3237;
    color: #EBEBEB;
    border: none;
    padding: 6px;
    border-radius: 3px;
}
QToolButton:hover {
    background-color: #3a3d43;
}
QToolButton:pressed {
    background-color: #5196F5;
    color: #1C1E21;
}

/* ========== Menu Bar ========== */
QMenuBar {
    background-color: #26282C;
    color: #EBEBEB;
}
QMenuBar::item:selected {
    background-color: #5196F5;
    color: #2F3237;
}

/* ========== Status Bar ========== */
QStatusBar {
    background-color: #1C1E21;
    color: #EBEBEB;
}

/* ========== Scroll Bars ========== */
QScrollBar:vertical, QScrollBar:horizontal {
    background: #1C1E21;
}
QScrollBar::handle {
    background: #5196F5;
    border-radius: 4px;
}

/* ========== Table Widget (QTableWidget) ========== */
QTableWidget {
    background-color: #1C1E21;
    alternate-background-color: #26282C;
    color: #EBEBEB;
    gridline-color: #2F3237;
    selection-background-color: #5196F5;
    selection-color: #1C1E21;
    border: 1px solid #2F3237;
}
QHeaderView::section {
